{
  "name": "remote",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "kill-port 6100 && vue-cli-service serve --port 6100",
    "build": "pnpm run build:client && pnpm run build:server",
    "build:client": "vue-cli-service build --dest dist/client",
    "build:server": "cross-env SSR=true vue-cli-service build --dest dist/server",
    "start": "kill-port 6100 && serve dist -p 6100",
    "lint": "vue-cli-service lint",
    "build-and-serve": "pnpm run build && pnpm run start"
  },
  "dependencies": {
    "core-js": "^3.8.3",
    "vue": "^3.2.13"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-typescript": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "kill-port": "^2.0.1",
    "serve": "^14.1.2",
    "typescript": "~4.5.5"
  }
}
